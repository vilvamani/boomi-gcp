resources:
{% for subnet in properties["subnets"] %}
  {% set name = subnet["name"] %}
  {% set range = subnet["range"] %}
  {% set region = subnet["region"] %}
- name: {{ name }}
  type: compute.v1.subnetwork
  properties:
    network: {{ properties["network"] }}
    ipCidrRange: {{ range }}
    region: {{ region }}
{% endfor %}
